public class exceptionhandling {
    public static void main(String[] args) {
        int a= 10; 
        int b=0;
    
    try{
        int c= a/b;
        System.out.println(c);

    } catch( Exception e){
         System.out.println("we can't divide by zero");
    }

    System.out.println("hello");
    System.out.println("hiiii");
    
}
}
